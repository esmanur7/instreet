{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Node", // Node module çözümlemesi
    "target": "ESNext",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "#app": ["./node_modules/nuxt/dist/app"],
      "#app/*": ["./node_modules/nuxt/dist/app/*"]
    },
    "types": ["@types/node", "nuxt"]
  }
}
